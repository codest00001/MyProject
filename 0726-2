if (true) {
    console.log("트루1성인입니다.");
}


if (false) {
    console.log("폴스2성인입니다.");
}

if ([]) {
    console.log("배열3성인입니다.");
}

if (10) {
    console.log("숫자4성인입니다.");
}

if (null) {
    console.log("널5성인입니다.");
}

if (undefined) {
    console.log("언디파인드6성인입니다.");
}

if ("10") {
    console.log("문자7성인입니다.");
}

if ({}) {
    console.log("빈8성인입니다.");
}